@{
Layout = "_AdminLayout";
}
@model List<Post>
<br />
<div style="text-align:center;">
<label style="font-size: 40px;">Posts</label>
</div>
<br />
<table id="Posts" class="table table-striped table-bordered dt-responsive nowrap" width="100%" cellspacing="0">
    <thead>
        <tr>
            <th>Post Id</th>
            <th>Account Id</th>
            <th>Post Image</th>
            <th>Created By</th>
            <th>Created Date</th>
            <th>Last ModifiedBy</th>
            <th>Last Modified Date</th>
        </tr>
    </thead>
    <tbody>
        @for (int i = Model.Count - 1; i >= 0; i--)
        {
            <tr>
                <td>@Model[i].PostId</td>
                <td>@Model[i].AccountId</td>
                <td>@Model[i].PostImage</td>
                <td>@Model[i].CreatedByUserId</td>
                <td>@Model[i].CreatedDate</td>
                <td>@Model[i].LastModifiedUserId</td>
                <td>@Model[i].LastModifiedDate</td>
            </tr>
        }
    </tbody>
    <tfoot>
        <tr>
            <th>Post Id</th>
            <th>Account Id</th>
            <th>Post Image</th>
            <th>Created By</th>
            <th>Created Date</th>
            <th>Last ModifiedBy</th>
            <th>Last Modified Date</th>
        </tr>
    </tfoot>
</table>